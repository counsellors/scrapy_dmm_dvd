<script>
function toDateStringFormat(date, format) {
    var date   = date instanceof Date ? date : new Date();
    var format = format ? format : 'yy-mm-dd';
    var year   = date.getFullYear();
    var month  = date.getMonth() + 1 < 10 ? '0' + (date.getMonth() + 1) : (date.getMonth() + 1);
    var day    = date.getDate() < 10 ? '0' + date.getDate() : date.getDate();

    return format.replace(/yy/, year).replace(/mm/, month).replace(/dd/, day);
}

function addDate(date, diff, type) {
    var date = date instanceof Date ? date : new Date();
    var diff = parseInt(diff) ? parseInt(diff) : 0;
    var type = type ? type : 'day';

    if (type == 'day') {
        date.setDate(diff + date.getDate());
    }
    if (type == 'month') {
        date.setMonth(diff + date.getMonth());
    }
    if (type == 'year') {
        date.setFullYear(diff + date.getFullYear());
    }

    return date;
}

function getWeekday(date) {
    var date    = date instanceof Date ? date : new Date();
    var weekday = ['日', '月', '火', '水', '木', '金', '土'];

    return weekday[date.getDay()];
}

function numberFormat(number) {
    var number = number ? String(number) : '0';
    return number.replace(/(\d)(?=(\d\d\d)+(?!\d))/g, '$1,' );
}

function sendI3Log(type, callback, cidList, option) {
    
    var callback = callback ? callback : function() {};
    var cid       = String('');
    var cidList   = cidList ? cidList : {};
    var option    = option  ? option  : {};

    if (type == 'detail') {
        var price        = parseInt();
        var sizeSchedule = JSON.parse('null');
        var logInfo     = { content_id: cid };
    } else if (type == 'add') {
        var productId = $('#reserve-schedule').find('input[name=product_id]').val();
        var price     = parseInt($('#reserve-rental-price').text().replace(',', '').replace('円', ''));
        var productInfo = { content_id: cid, product_id: productId, price: price, has_stock: true };
    } else if (type == 'recommend') {
        var recommend     = JSON.parse('null');
        var hitCount      = parseInt(0);
        var recommendInfo = { type: '', hit_count: hitCount, page_number: 1, limit: 60, show_count: 5};
    } else if (type == 'show') {
        var contentInfo = { content_id: cidList[0] };
    }

    // トラッカーの生成
    if (type == 'detail') {
        i3('create', 'detail');
    } else if (type == 'add') {
        i3('create', 'cart');
    } else if (type == 'recommend') {
        i3('create', 'rec1');
    } else if (type == 'show') {
        i3('create', 'log');
    } else if (type == 'click' && option == 'favo') {
        i3('create', 'favo');
    } else if (type == 'click' && option == 'cart') {
        i3('create', 'cart');
    } else if (type == 'click' && option == 'wish') {
        i3('create', 'wish');	
	}

    // ログ情報の入力
    if (type == 'detail') {
        i3('detail.set', 'detail', logInfo);
    } else if (type == 'recommend') {
        i3('rec1.set', 'recommend', recommendInfo);
    } else if (type == 'show') {
        i3('log.set', 'detail', contentInfo);
    }

    // 商品情報リストの追加
    if (type == 'detail') {
        JSON.parse('null', function(sizeId, productId) {
            var hasStock =sizeSchedule[sizeId] ? true : false;
            var productInfo = { product_id: productId, price: price, has_stock: hasStock };
            if (!$.isEmptyObject(productId)) {
                i3('detail.add', 'detail', productInfo);
            }
        });
    } else if (type == 'add') {
        i3('cart.add', 'detail', productInfo);
    } else if (type == 'recommend') {
        $.each(recommend, function(num, data) {
            var productInfo = { content_id: data['cid'], product_id: data['product_id'], price: data['price'], has_stock: true };
            i3('rec1.add', 'recommend', productInfo);
        });
    } else if (type == 'click' && option == 'favo') {
        for (var i = 0; i < cidList.length; i++) {
            var contentInfo = { content_id: cidList[i] };
            i3('favo.add', 'detail', contentInfo);
        }
    } else if (type == 'click' && option == 'cart') {
        for (var i = 0; i < cidList.length; i++) {
            var contentInfo = { content_id: cidList[i] };
            i3('cart.add', 'detail', contentInfo);
        }
    } else if (type == 'click' && option == 'wish') {
        for (var i = 0; i < cidList.length; i++) {
            var contentInfo = { content_id: cidList[i] };
            i3('wish.add', 'detail', contentInfo);
        }
    }

    // ログの送信
    if (type == 'detail') {
        i3('detail.send', 'show', 'detail', function() {
            callback();
        });
    } else if (type == 'add') {
        i3('cart.send', 'click', 'add_cart', function() {
            callback();
        });
    } else if (type == 'recommend') {
        i3('rec1.send', 'show', 'recommend');
    } else if (type == 'show') {
        i3('log.send', 'show', 'detail', function() {
            callback();
        });
    } else if (type == 'click' && option == 'favo') {
        i3('favo.send', 'click', 'add_favorite', function() {
            callback();
        });
    } else if (type == 'click' && option == 'cart') {
        i3('cart.send', 'click', 'add_cart', function() {
            callback();
        });
    } else if (type == 'click' && option == 'wish') {
        i3('wish.send', 'click', 'add_rental_list', function() {
            callback();
        });
    }

}

function sendGoogleAnalytics(type) {
    var deferred = new $.Deferred();
    var cid      = String('');
    var gaType   = '';

    switch (type) {
        case 'reserve_button':
            gaType = 'reservation';
            break;

        case 'reserve_calendar':
            gaType = 'calender_available';
            break;

        case 'reserve_submit':
            gaType = 'basket';
            break;

        case 'alert_button':
            gaType = 'restock';
            break;

        case 'alert_calendar':
            gaType = 'calender_not_available';
            break;

        case 'alert_submit':
            gaType = 'notice_restock';
            break;

        case 'reminder_button':
            gaType = 'reservation_start';
            break;

        case 'reminder_calendar':
            gaType = 'calender_before_reserve';
            break;

        case 'reminder_submit':
            gaType = 'notice_reservation_start';
            break;

        default:
            break;
    }
    if (!cid || !gaType) {
        deferred.reject();
        return deferred.promise();
    }

    ga('mutual.send', 'event', 'detail_iro_rental', gaType, cid);

    deferred.resolve();
    return deferred.promise();
}


</script>
        <script>
                var j$ = jQuery;
        j$("form p._add_btn").each(function(i, e) {
            var id = j$(e).attr('id');
            var btn = j$(e).children('input:first')[0];
            var cid = j$(e).attr('cid');
            if (!id || !btn) next;
            $(btn).click(function() {
                sendI3Log('click', null, new Array(cid), 'wish');return true;            });
        });
        </script>
